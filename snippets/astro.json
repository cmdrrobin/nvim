{
    "astro_post": {
        "prefix": "astro_post",
        "body": [
            "---",
            "pubDatetime: $2",
            "title: $3",
            "postSlug: $4",
            "featured: false",
            "draft: true",
            "tags:",
            "\t- $3",
            "description:",
            "\t $4",
            "---",
            "$0"
        ],
        "description": "Astro syntax for creating a post in Markdown"
    }
}
